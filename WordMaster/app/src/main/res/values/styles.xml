<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#141a46</item>
        <item name="colorPrimaryDark">#1A225C</item>
        <item name="colorAccent">#EC8B5E</item>
    </style>
    <style name="text_normal_style">
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textColor">#EC8B5E</item>
        <item name="android:textSize">@dimen/text_normal_size</item>
    </style>
    <style name="text_reg_style">
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textColor">#141a46</item>
        <item name="android:textSize">@dimen/text_reg_size</item>
    </style>
    <style name="button_style">
        <item name="android:background">#EC8B5E</item>
        <item name="android:textSize">@dimen/text_normal_size</item>
        <item name="android:textColor">#141a46</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textAllCaps">false</item>
    </style>

</resources>
